onerror {resume}
quietly WaveActivateNextPane {} 0
add wave -noupdate /tb_edge_detector/clk
add wave -noupdate /tb_edge_detector/rst_n
add wave -noupdate -divider {DuT - In}
add wave -noupdate /tb_edge_detector/din
add wave -noupdate /tb_edge_detector/dout
